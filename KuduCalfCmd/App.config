<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <switches>
      <add name="sourceSwitch" value="Information"/>
    </switches>
    <sharedListeners>
      <add name="ConsoleListener"  type="System.Diagnostics.ConsoleTraceListener"/>
    </sharedListeners>
    <sources>
      <source name="SmartSync" switchName="sourceSwitch">
        <listeners>
          <clear />
          <add name="ConsoleListener"/>
        </listeners>
      </source>
      <source name="KuduCalfCmd" switchName="sourceSwitch">
        <listeners>
          <clear />
          <add name="ConsoleListener"/>
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>